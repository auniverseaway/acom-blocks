{
    "extends": "airbnb-base",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true,
        "requireConfigFile": false,
    },
    "globals": {
        "document": true,
        "window": true,
        "Element": true,
        "fetch": true,
        "HTMLElement": true,
        "HTMLDocument": true,
        "IntersectionObserver": true,
        "MutationObserver": true
    },
    "rules": {
        "indent": ["error", 4]
    }
}
